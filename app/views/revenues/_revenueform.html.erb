<%= simple_form_for [@contract, @revenue] do |f| %>
      <%= f.error_notification %>
      <div class="form-actions">
        <%= f.button :submit, :class => "submit-btn" %>
      </div>

      <div class="form-inputs">
        <%= f.input :rent_month, order: [:month, :year] %>
        <%= f.input :paid %>
       
      </div>

      <div class="form-actions">
        <%= f.button :submit, :class => "submit-btn" %>
      </div>
    <% end %>