 <table class="table table-responsive table-striped ">
    <thead>
      <tr>
        <th>Rent start</th>
        <th>Rent end</th>
        <th>Renter</th>
        <th>Rent amount</th>
        <th>Warranty amount</th>
        <th>Pay day</th>
        <th>Active</th>
        <th>Days to reminder</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @contracts.each do |contract| %>
        <tr>
          <td><%= contract.rent_start %></td>
          <td><%= contract.rent_end %></td>
          <td><%= contract.renter.first_name + ' ' + contract.renter.last_name %></td>
          <td><%= contract.rent_amount %></td>
          <td><%= contract.warranty_amount %></td>
          <td><%= contract.pay_day %></td>
          <td><%= contract.active %></td>
          <td><%= contract.days_to_reminder %></td>
         </tr>
      <% end %>
    </tbody>
  </table>
