<div class="form-inputs">
  <%= simple_form_for Renter.new, url: contact_form_path do |f| %>
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :email %>
    <%= f.input :gender, label: "Your question" %>
    <%= f.button :submit, class: "submit-btn", value: "Send your question" %>
  <% end %>
</div>
